{
    "compilerOptions": {
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "lib": [
            "es2016"
        ],
        "outDir": "./../client/server",
        "alwaysStrict": true
    },
    "exclude": [
        "node_modules"
    ]
}
