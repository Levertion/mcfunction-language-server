{
    "name": "mcfunction-language-server",
    "description": "An implementation of the language server protocol for Minecraft Commands.",
    "version": "0.0.1",
    "author": "Levertion",
    "license": "MIT",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Levertion/mcfunction-language-server"
    },
